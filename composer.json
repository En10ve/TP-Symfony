{
    "name": "myvendor/reservation_system",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "symfony/asset": "^7.2",
        "symfony/asset-mapper": "*",
        "symfony/console": "^7.2",
        "symfony/dotenv": "^7.2",
        "symfony/form": "*",
        "symfony/framework-bundle": "^7.2",
        "symfony/mailer": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/orm-pack": "*",
        "symfony/runtime": "*",
        "symfony/security-bundle": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/translation": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/ux-turbo": "^2.22",
        "symfony/validator": "^7.2",
        "symfony/yaml": "^7.2",
        "twig/extra-bundle": "*",
        "twig/twig": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony console cache:clear",
            "assets:install %PUBLIC_DIR%": "symfony console assets:install"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^7.2",
        "symfony/css-selector": "^7.2",
        "symfony/debug-bundle": "^7.2",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/stopwatch": "^7.2",
        "symfony/web-profiler-bundle": "^7.2"
    }
}
